#pragma rtGlobals=1		// Use modern global access method.

//-------------------------------------------------------------------------------------------------------------------------------------------------------------

Function Size_dist(w,D) : FitFunc	// Weibull
	Wave/D w
	Variable D

	Variable fwhm=w[3]
	
	Return fwhm/w[2]*(D/w[2])^(fwhm-1)*exp(-(D/w[2])^(fwhm))
End

